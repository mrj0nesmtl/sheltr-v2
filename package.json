{
  "name": "sheltr-v2",
  "private": true,
  "version": "0.4.0-beta",
  "type": "module",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.19.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules/.cache dist .vite",
    "clean:all": "rm -rf node_modules/.cache dist .vite && npm cache clean --force && rm -rf node_modules",
    "start": "vite",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "deploy:staging": "npm run build:staging && npm run test",
    "deploy:prod": "npm run build && npm run test",
    "analyze": "vite build --mode analyze || echo 'Analysis completed with warnings'",
    "increment-version": "node scripts/increment-version.js",
    "update-docs": "node scripts/update-docs.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@stripe/stripe-js": "^3.0.10",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.8",
    "@tanstack/react-table": "^8.13.2",
    "@types/mapbox-gl": "^3.4.1",
    "@types/qrcode": "^1.5.5",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.13.1",
    "gray-matter": "^4.0.3",
    "html5-qrcode": "^2.3.8",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "js-yaml": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "mapbox-gl": "^3.8.0",
    "marked": "^15.0.4",
    "nanoid": "^5.0.9",
    "openai": "^4.76.3",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.0",
    "react-i18next": "^14.1.3",
    "react-leaflet": "^4.2.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.14.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "tailwind-merge": "^2.2.1",
    "unified": "^11.0.5",
    "zod": "^3.24.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/js-yaml": "^4.0.9",
    "@types/leaflet": "^1.9.8",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
