{
  "ts-node": {
    "esm": true,
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "bundler"
    }
  },
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "typeRoots": ["./node_modules/@types"],
    "types": [
      "node",
      "geojson"
    ]
  },
  "include": ["vite.config.ts"]
}