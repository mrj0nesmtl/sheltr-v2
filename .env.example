# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-anon-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# API Configuration
VITE_API_URL=https://sheltr-ops.replit.app
VITE_PUBLIC_URL=https://sheltr-ops.replit.app

# Environment
VITE_ENVIRONMENT=production
VITE_PORT=5173

# Feature Flags
VITE_ENABLE_MONITORING=true
VITE_SECURITY_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CACHING=true
VITE_ENABLE_QR_SCANNER=true
VITE_ENABLE_OFFLINE_MODE=false

# Performance
VITE_API_TIMEOUT=30000
VITE_RATE_LIMIT=100

# Supabase Database Configuration
SUPABASE_DB_HOST=aws-0-ca-central-1.pooler.supabase.com
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres.your_project_ref 